DROP TABLE PROJECT;
CREATE TABLE PROJECT (
  PRJNO NUMBER NOT NULL,
  PRJNAME VARCHAR2(50) NOT NULL,
  PRJST DATE,
  PRJEND DATE,
  PRJDESC VARCHAR2(1000),
  PRJSTATUS VARCHAR2(10),
  VNO NUMBER
);

commit;

